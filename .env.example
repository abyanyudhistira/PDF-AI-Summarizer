# Copy file ini jadi .env dan isi dengan value yang sesuai

# AI Service - Gemini API Key (WAJIB DIISI!)
GEMINI_API_KEY=your_gemini_api_key_here

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=admin
DB_PASSWORD=admin123
